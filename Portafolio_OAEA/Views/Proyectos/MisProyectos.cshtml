@model List<ProyectoModel>;

    <!DOCTYPE html>
    <html>
    <head>
        <meta charset="UTF-8">
        <title>Listado de Proyectos</title>
        <link rel="stylesheet" href="~/css/StyleSheetProyecto.css">
    </head>
    <body>
        <h1>Listado de Proyectos</h1>

        <table>
            <thead>
                <tr>
                    <th>Nombre</th>
                    <th>Descripción</th>
                    <th>Fecha</th>
                </tr>
            </thead>

          <tbody>
        @foreach (var item in Model)
        {
                <tr>
                    <td>@item.NombreProyecto</td>
                    <td>@item.Descripcion</td>
                    <td>@item.FechaCreado</td>
                    
                    <!-- Agrega más columnas según sea necesario -->
                </tr>
        }
                 </tbody>
                <!-- Puedes iterar sobre los proyectos y generar estas filas dinámicamente -->
            </tbody>
        </table>

        <p><a href="/">Volver al inicio</a></p>
    </body>
    </html>
